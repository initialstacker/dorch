{
    "name": "initialstacker/dorch",
    "description": "Laravel Doctrine ORM connector",
    "keywords": ["laravel", "doctrine"],
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "doctrine/orm": "^2.14",
        "doctrine/dbal": "^3.3",
        "symfony/cache": "^6.1 || ^6.2 || ^6.3",
        "ramsey/uuid": "^4.9"
    },
    "conflict": {
        "symfony/http-foundation": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Initialstacker\\Dorch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Initialstacker\\Dorch\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Initialstacker\\Dorch\\ServiceProvider",
                "Initialstacker\\Dorch\\DoctrineConnector"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}