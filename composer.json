{
  "name": "initialstacker/dorch",
  "description": "Laravel Doctrine ORM connector",
  "keywords": ["laravel", "doctrine"],
  "type": "library",
  "license": "MIT",
  "version": "1.0.1",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0",
    "doctrine/orm": "^2.18",
    "doctrine/dbal": "^3.2",
    "doctrine/lexer": "^2.1",
    "ramsey/uuid": "^4.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5.3"
  },
  "autoload": {
    "psr-4": {
      "Initialstacker\\Dorch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Initialstacker\\Dorch\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Initialstacker\\Dorch\\ServiceProvider",
        "Initialstacker\\Dorch\\DoctrineConnector"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}